# Patcher Server Configuration

# Server settings
PORT=8080
DEBUG=false
LOG_LEVEL=INFO

# GitHub App settings
GITHUB_APP_ID=
GITHUB_APP_PRIVATE_KEY_PATH=./private-key.pem
GITHUB_WEBHOOK_SECRET=

# LLM Provider: "claude" or "openai"
LLM_PROVIDER=claude

# API Keys (set one based on LLM_PROVIDER)
ANTHROPIC_API_KEY=
OPENAI_API_KEY=

# Agent settings
MAX_ITERATIONS=5
AUTO_MERGE=false
